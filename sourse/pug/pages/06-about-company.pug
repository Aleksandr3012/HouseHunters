extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug
include ../blocks/sAside/_sAside.pug
include ../blocks/sMainPage/_sMainPage.pug
include ../blocks/sCatalog/_sCatalog.pug
include ../blocks/topBuy/_topBuy.pug
include ../blocks/sChosenObj/_sChosenObj.pug
include ../blocks/pageHead/_pageHead.pug
include ../blocks/dayInCompany/_dayInCompany.pug
include ../blocks/sForm/_sForm.pug
include ../blocks/sCompanyMission/_sCompanyMission.pug
include ../blocks/sCompanyTargets/_sCompanyTargets.pug
include ../blocks/sDigits/_sDigits.pug
include ../blocks/sOurEmployee/_sOurEmployee.pug
include ../blocks/sOffice/_sOffice.pug

block variables
	- let  mainPage= null
block title
	- let  title= '06-about'
	title= title
block pagecontent
	.main-wrapper
		+Aside()(class='white-aside')
		+header()
			+infoStrip()
			+top-nav()(class='with-white-aside')
		//-+sChosenObj()
		+pageHead()
			style.
				@media (min-width: 576px) {
					.pageHead {
						background: linear-gradient(rgba(0, 0, 0, .7), rgba(0, 0, 0, .7)), no-repeat center/cover url(img/@2x/about-company-page-head.jpg);
					}
				}
			.container.text-center
				+b.section-title
					h2
						| Агентство недвижимости &laquo;HouseHunters&raquo;&nbsp;&mdash; это&nbsp;профессионалы, выросшие
						|
						br.d-none.d-xl-block
						| из&nbsp;любителей своего дела!
				+e.A.default-btn.scroll-vanilla-js(href="#sForm") Узнать больше
		+dayInCompany()
		+sForm()
		+sCompanyMission()
		+sCompanyTargets()
		+sDigits()
		+sOurEmployee()
		+sOffice()
		+footer()