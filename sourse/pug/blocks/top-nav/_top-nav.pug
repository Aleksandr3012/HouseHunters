mixin top-nav() 
	// start top-nav
	+b.top-nav.block-with-lazy&attributes(attributes)
		+e.A.logo(href="/")
			block
		+e.container.container
			+e.A.location-link(href="#")
				+icon('location')
				span Санкт-Петербург
				+icon('chevron-down')
			+e.A.mail-link(href='mailto:agentstvo@gmail.com')
				+icon('mail')
				span agentstvo@gmail.com
			+e.A.phone-link(href='tel:+79053454212')
				+icon('phone')
				span +7 (905) 345-42-12
			+e.A.consult-btn(href="#") Бесплатная консультация
			+e.icons-box
				a(href="#")
					+icon('heart')
				a(href="#")
					+icon('user')
				//location
	// end top-nav
mixin infoStrip()
	+b.info-strip
		+e.address-gray Адрес офиса:
		+e.address Санкт-Петербург, Воскресенская 12
		+e.shedule ПН-ПТ с 9:00 до 20:00
		+e.A.add-btn(href="#")
			+icon('plus')
			span Добавить объект