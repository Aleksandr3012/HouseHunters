								+select2PHLess('room', ['2-комнатную', '3-комнатную', '4-комнатную'])
							+e.select2-cont.price-select
								+select2PHLess('room', ['Цена', 'Цена2', 'Цена3 Цена3'])
							+input('Адрес', 'text', undefined, 'price')
						+e.BUTTON.submit-btn(type='submit')
							+icon('search')
							span Показать 2327 объектов
					+e.A.call-btn(href="#")
						+icon('phone')
