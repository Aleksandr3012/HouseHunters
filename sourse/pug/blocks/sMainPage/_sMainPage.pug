mixin sMainPage()
	// start sMainPage
	+b.SECTION.sMainPage.section#sMainPage&attributes(attributes)
		+pic('main-page-bg.jpg')(class="sMainPage__bg")
		+b.page-content
			.container
				block
				+b.section-title
					h2 Недвижимость в Санкт-Петербурге
				+b.buy-sel-form
					+form-wrap()
						+e.top-btns
							+topBtn('Купить')(checked)
							+topBtn('Продать')
							+topBtn('Снять')
							+topBtn('Сдать')
